<% if current_user %>
<%= link_to "Log Out", logout_path, method: :get %>
<section class='existing-users'>
    <p>Existing Users: </p>
    <% @users.each do |user| %>
        <li><%= user.email %></li>
    <% end %>
</section>
<% else %>
<%= link_to "I already have an account", login_path %>
<%= button_to "Create New User", register_path, method: :get %>
<% end %>